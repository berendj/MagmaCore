{
    "name": "berendj/magmacore",
    "description": "A simple PHP framework for building web applications",
    "keywords": ["framework", "php", "magmacore"],
    "type": "library",
    "license": "MIT",
    "readme": "",
    "time": "",
    "support": {
        "issues": "https://github.com/berendj/MagmaCore/issues",
        "source": "https://github.com/berendj/MagmaCore",
        "wiki": "https://github.com/berendj/MagmaCore/wiki"
    },
    "autoload": {
        "psr-4": {
            "Magma\\": "src/Magma/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Magma\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "BJ Ruiterkamp",
            "email": "bj.ruiterkamp@hccnet.nl"
        }
    ],
    "minimum-stability": "alpha",
    "require": {
        "php": "7.4.33|^8.0",
        "ext-mbstring": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "twig/twig": "^3.22",
        "symfony/http-foundation": "^7.4"
    }
}
